{
  "name": "r9s",
  "version": "0.1.0",
  "description": "Official TypeScript SDK for r9s.ai",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "RELEASE.md",
    "LICENSE"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && tsc -p tsconfig.json",
    "dev": "tsc -w -p tsconfig.json",
    "prepack": "npm run build",
    "test": "node -e \"console.log('No tests specified')\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/r9s-ai/r9s-typescript.git"
  },
  "homepage": "https://github.com/r9s-ai/r9s-typescript",
  "bugs": {
    "url": "https://github.com/r9s-ai/r9s-typescript/issues"
  },
  "keywords": [
    "r9s",
    "r9s-ai",
    "edge-functions",
    "sdk",
    "typescript",
    "api-client"
  ],
  "author": "r9s-ai",
  "license": "MIT",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "type": "commonjs",
  "devDependencies": {
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=18"
  }
}
