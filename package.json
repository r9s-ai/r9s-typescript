{
  "name": "@r9s/sdk",
  "version": "0.5.0",
  "workspaces": [
    ".",
    "examples/*"
  ],
  "description": "Official TypeScript SDK for r9s.ai",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "RELEASE.md",
    "LICENSE"
  ],
  "scripts": {
    "lint": "eslint --cache --max-warnings=0 src",
    "clean": "rm -rf dist",
    "build": "npm run clean && tsc -p tsconfig.json",
    "dev": "tsc -w -p tsconfig.json",
    "prepack": "npm run build",
    "test": "node -e \"console.log('No tests specified')\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/r9s-ai/r9s-typescript.git"
  },
  "homepage": "https://github.com/r9s-ai/r9s-typescript",
  "bugs": {
    "url": "https://github.com/r9s-ai/r9s-typescript/issues"
  },
  "keywords": [
    "r9s",
    "r9s-ai",
    "edge-functions",
    "sdk",
    "typescript",
    "api-client"
  ],
  "author": "r9s-ai",
  "license": "MIT",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "type": "commonjs",
   "devDependencies": {
    "@eslint/js": "^9.19.0",
    "eslint": "^9.19.0",
    "globals": "^15.14.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.26.0"
  },
    "dependencies": {
    "zod": "^3.25.65 || ^4.0.0"
  },
  "engines": {
    "node": ">=18"
  }
}
